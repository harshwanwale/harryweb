{
    "version":2,
    "builds":[
        {
           "src":"webapp/wsgi.py",
           "use":"@vercel/python",
           "config":{
                      "maxLambdaSize":"15mb",
                      "runtime":"python3.11.1"
                    }
        },
        {
        "src":"build.sh",
        "use":"@vercel/static-build",
        "config":{
            "distDir":"staticfiles_build"
        }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {   
            "src":"/(.*)",
            "dest":"webapp/wsgi.py"

        }
    ]
    
}